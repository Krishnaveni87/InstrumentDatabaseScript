    <style>

        table {
            border-collapse: collapse;
            width: 100%;
        }

            table thead tr th {
                text-transform: capitalize;
            }

            table thead tr:last-child {
                background: #eaeaea;
                border-bottom: 4px double #cbcbcb;
            }



            table tbody tr:nth-child(even) {
                background-color: #f2f2f2;
            }

            table tbody tr.hidden-row {
                display: none;
            }


        .tableFixHead {
            overflow: auto;
            height: 450px;
            display: block;
            width: auto;
        }

            .tableFixHead thead tr th input {
                width: 100%;
            }

            .tableFixHead thead, .tableFixHead tbody {
                width: 100%;
            }

                .tableFixHead thead tr:first-child th {
                    position: sticky;
                    top: 0;
                    background: lightblue;
                }

        table.dataTable thead th, table.dataTable thead td {
            background: lightblue;
        }



        .dataTables_scrollHeadInner {
            width: 100%;
        }

        div#example_filter input,
        div#example1_filter input,
        div#example2_filter input,
        div#example3_filter input,
        div#example4_filter input,
        div#example5_filter input,
        div#example6_filter input,
        div#example7_filter input,
        div#exampleTemp_filter input {
            background-color: snow;
        }

        table.display th {
            font-size: 16px;
        }

        table.display td {
            font-size: 14px;
        }





        dataTables_scrollBody {
            width: auto;
        }

        datatables_scrollHeadInner {
            padding-right: 0px;
        }

        .tab-ctn {
            display: table;
            margin: auto;
        }
    </style>
